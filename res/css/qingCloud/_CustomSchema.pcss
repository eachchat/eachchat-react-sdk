.mx_Custom_schema {
    width: calc(100% - 30px);
    padding: 20px 0;
    color:  $primary-content !important;
   .button-group{
    display: flex;
    align-items: center;
    justify-content: space-evenly;
   }
   input{
        height: 32px;
        margin: 0 !important;
        padding: 0 11px;
        color:  $primary-content !important;
   }
   .grid {
        display: grid;
    }
    .gap-10{
        gap: 0px 10px;
    }
    .ant-form-item{
        margin-bottom: 8px;
    }
}